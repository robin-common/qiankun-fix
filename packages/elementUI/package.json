{
  "name": "@qiankun-fix/element-ui",
  "version": "0.8.3",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup -c -w",
    "publish": "npm publish",
    "release": "release-it"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "author": "RobinWM",
  "keywords": [
    "qiankun",
    "微前端",
    "element-UI",
    "vue"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "release-it": "^15.10.3",
    "rollup": "^3.22.0"
  },
  "files": [
    "dist/*",
    "*.json",
    "*.md"
  ],
  "release-it": {
    "node": {
      "release": true
    },
    "github": {
      "release": false
    },
    "git": {
      "requireCleanWorkingDir": false,
      "commitMessage": "chore(tag): release v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "✨新功能"
            },
            {
              "type": "fix",
              "section": "🐛问题修复"
            },
            {
              "type": "docs",
              "section": "📚文档"
            }
          ]
        },
        "infile": "CHANGELOG.md",
        "ignoreRecommendedBump": true,
        "strictSemVer": true
      }
    }
  }
}
